<!doctype html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0">

    <title>{% block title %}Welcome!{% endblock %}</title>

    {% stylesheets
        '@WebicksMutteryBundle/Resources/public/css/responsive-style.css'
        '@WebicksMutteryBundle/Resources/public/css/style.css'
        filter='cssrewrite'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    <script type="text/javascript">
    {% if app.user %}
        var user = {{ '{' }} 'userid': '{{ app.user }}',
        'groups': {{ app.user.getRoles|json_encode()|raw }} {{ '}' }};
    {% else %}
        var user = false;
    {% endif %}
    {% block headJS %}
    {# Enable sub-templates to inject header js #}
    {% endblock %}
    </script>

	{% javascripts
		'@WebicksJqueryBundle/Resources/public/jquery-1.8.2.min.js'
		'@WebicksJqueryBundle/Resources/public/jquery.tools.min.js'
		'@WebicksJqueryBundle/Resources/public/jquery.cookie.js'
		'@WebicksMutteryBundle/Resources/public/js/init.js'
	%}
		<script type="text/javascript" src="{{ asset_url }}" ></script>
	{% endjavascripts %}

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body class="page page-id-1484 page-template page-template-responsive-theme-php">
{{ facebook_initialize({'xfbml': true, 'fbAsyncInit': '$(document).trigger(\'fbInit\');'}) }}

    <div id="container" class="hfeed">
        <div id="header">
            <div id="logo">
                <a href="http://muttery.com/"><img src="{{ asset("bundles/webicksmuttery/images/logo.png") }}" width="300" alt="ThemeID" /></a>
            </div><!-- end of #logo -->
    </div><!-- end of #header -->
    <div id="wrapper" class="clearfix">
        <div id="content-full" class="grid col-940">
        {% block body %}{#         Here goes your content!     #}{% endblock %}
        </div><!-- end of content -->
    </div><!-- end of #wrapper -->
</div><!-- end of #container -->

<div id="footer" class="clearfix">
    <div id="footer-wrapper">
        <div class="grid col-940">
            <div class="grid col-540">
            </div>
            <div class="grid col-380 fit">
            </div>
        </div>
        <div class="grid col-300 copyright">
           <a href="http://themeid.com/">
                <img src="{{ asset("bundles/webicksmuttery/images/footer-logo.png") }}" class="logo-footer" width="100" height="26" alt="ThemeID" />
           </a>
        </div>
        <div class="grid col-300 fit powered">
        </div><!-- end .powered -->
    </div><!-- end #footer-wrapper -->
</div><!-- end #footer -->
</body>
</html>